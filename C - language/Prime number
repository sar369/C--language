#include <stdio.h>
void main()
{
    int n,count,i;
    printf("Enter n: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n%i == 0)
        {
            count++;
        }
    }
    if(count == 2)
    {
        printf("Prime number");
    }
    else
    {
        printf("Not a Prime number");
    }
}
